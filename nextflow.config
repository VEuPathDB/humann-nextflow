params {
  inputPath = "$baseDir/data/sample-to-fastqs.tsv"
  resultDir = "$baseDir/results"
  kneaddataCommand = "kneaddata"
  libraryLayout = 'paired'
  humannCommand = "humann"
  unirefXX = "uniref90"
  functionalUnits = [ "eggnog", "go", "ko", "level4ec", "pfam", "rxn" ]
  apiKey = ''
  humann_databases = "$launchDir/data/humann_databases"
}
process {
  withLabel: 'download_and_preprocess' {
    maxForks = 5
  }
  container = 'veupathdb/humann:latest'
}
docker {
  enabled = true
}